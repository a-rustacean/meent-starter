{
  "name": "meent-starter",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node .",
    "start": "NODE_ENV=production node .",
    "format": "prettier --write .",
    "build": "postcss public/css/tailwind.css -o public/css/index.css",
    "build:watch": "postcss public/css/tailwind.css -o public/css/index.css --watch",
    "lint": "eslint .",
    "install-husky": "husky install"
  },
  "keywords": [],
  "author": "Dilshad <dilshadplayingminecraft@outlook.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-flash": "^0.0.2",
    "express-log-url": "^1.6.0",
    "express-session": "^1.18.2",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.2",
    "mongoose": "^8.17.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11"
  }
}
