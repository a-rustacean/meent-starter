{
  "name": "meent-starter",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node .",
    "start": "NODE_ENV=production node .",
    "format": "prettier --write .",
    "build": "postcss public/css/tailwind.css -o public/css/index.css",
    "build:watch": "postcss public/css/tailwind.css -o public/css/index.css --watch",
    "lint": "eslint .",
    "install-husky": "husky install"
  },
  "keywords": [],
  "author": "Dilshad <dilshadplayingminecraft@outlook.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "argon2": "^0.40.1",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.8",
    "express": "^4.18.3",
    "express-flash": "^0.0.2",
    "express-log-url": "^1.5.1",
    "express-session": "^1.18.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.2",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.11",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "cssnano": "^6.0.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1"
  }
}
